{
  "name": "structura",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "generate:i18n-types": "echo 'not implemented tsx scripts/generate-i18n-types.ts'",
    "dev": "next dev | pino-pretty",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "gen:types": "tsx scripts/generateSupabaseTypes.ts",
    "prepare": "pnpm dlx husky@latest install",
    "pre-commit": "lint-staged",
    "db:backup:structure": "tsx scripts/backup-database.ts --schema-only",
    "db:backup:full": "tsx scripts/backup-database.ts --with-data",
    "db:wipe": "tsx scripts/wipe-database.ts",
    "db:restore": "tsx scripts/restore-database.ts",
    "db:migrations:sync": "tsx scripts/sync-migrations.ts",
    "db:init": "tsx scripts/init-database.ts"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write --ignore-unknown",
      "eslint --fix --max-warnings=0"
    ],
    "*.{json,css,scss,md,mdx,html,yml,yaml}": [
      "prettier --write --ignore-unknown"
    ]
  },
  "dependencies": {
    "@datadog/browser-rum": "^6.25.1",
    "@datadog/browser-rum-react": "^6.25.1",
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@hookform/resolvers": "^3.3.2",
    "@mui/icons-material": "^7.3.4",
    "@mui/material": "^7.3.4",
    "@mui/material-nextjs": "^7.3.3",
    "@mui/material-pigment-css": "^7.3.3",
    "@mui/x-date-pickers": "^8.14.0",
    "@pigment-css/react": "^0.0.30",
    "@sentry/nextjs": "^10.30.0",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "latest",
    "@tanstack/react-query": "^5.90.11",
    "@tanstack/react-query-devtools": "^5.91.1",
    "@vercel/analytics": "latest",
    "@vercel/kv": "^3.0.0",
    "clsx": "^2.1.1",
    "cookie": "^1.0.2",
    "country-state-city": "^3.2.1",
    "date-fns": "4.1.0",
    "dompurify": "^3.3.1",
    "esbuild-plugin-pino": "^2.3.3",
    "eslint-config-next": "^16.0.10",
    "i18next": "^25.6.0",
    "i18next-browser-languagedetector": "^8.2.0",
    "isomorphic-dompurify": "^2.34.0",
    "jsdom": "^27.3.0",
    "lru-cache": "^11.2.2",
    "moment-timezone": "^0.6.0",
    "motion": "^12.23.22",
    "next": "15.5.6",
    "next-themes": "latest",
    "pino": "^10.0.0",
    "pino-datadog-transport": "^3.0.6",
    "pino-http": "^11.0.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "^7.45.4",
    "react-i18next": "^16.0.1",
    "react-phone-input-2": "^2.15.1",
    "tailwind-merge": "^2.5.5",
    "tailwindcss": "^4.1.14",
    "vanilla-cookieconsent": "^3.1.0",
    "zod": "3.25.67"
  },
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.39.1",
    "@jest/globals": "^30.2.0",
    "@next/eslint-plugin-next": "^16.0.4",
    "@tailwindcss/postcss": "^4.1.14",
    "@types/jest": "^30.0.0",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^22",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "autoprefixer": "^10.4.20",
    "chokidar": "^4.0.3",
    "dotenv": "^16.4.5",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "lint-staged": "^16.2.3",
    "pino-pretty": "^13.1.2",
    "postcss": "^8.5.6",
    "prettier": "^3.3.3",
    "ts-jest": "^29.4.5",
    "tsx": "^4.7.1",
    "typescript": "^5"
  }
}
